@using MesaSolidariaApi.Core.Models

@{
    string? returnUrl = null;
    var query = ViewContext.HttpContext.Request.Query;
    if (query.ContainsKey("returnUrl"))
    {
        returnUrl = query["returnUrl"];
    }
}

<ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="Delivery" asp-page="/Account/Register" asp-route-returnUrl="@returnUrl">Register</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-page="/NotificarEntrega">Notificar Entrega</a>
        </li>
</ul>